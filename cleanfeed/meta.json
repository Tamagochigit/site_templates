{
  "information": {
    "id": "cleanfeed",
    "name": "Clean Feed",
    "description": "A clean blog format using RSS",
    "preview_image_path": "/preview.png"
  },
  "fields": [
  {
	"id": "fullName",
	"label": "Full Name",
	"type": "text",
	"placeholder": "Your Name"
  },
  {
    "id": "siteTitle",
    "label": "Site Title",
    "type": "text",
    "placeholder": "Your Site Title"
  },
  {
    "id": "tagLine",
    "label": "Tag Line",
    "type": "text",
    "placeholder": "Tag Line"
  },
  {
	"id": "siteUrl",
	"label":"RSS Feed (must be valid feed URL)",
	"type": "url",
	"placeholder": "http://myblog.wordpress.com/feed"
  },
  {
    "id": "feedItemNumber",
    "label": "Number of Posts (max limited by RSS Feed)",
    "type": "number",
    "placeholder": "5"
  },
  {
    "id": "emailAddress",
    "label": "Email Address",
    "type": "email",
    "placeholder": "testemail@testemail.com"
  },
  {
    "id": "facebookLink",
    "label": "Facebook",
    "type": "url",
    "placeholder": "https://www.facebook.com/username"
  },
  {
    "id": "twitterLink",
    "label": "Twitter",
    "type": "url",
    "placeholder": "https://twitter.com/username"
  },
  {
    "id": "linkedinLink",
    "label": "LinkedIn",
    "type": "url",
    "placeholder": "https://www.linkedin.com/in/username"
  },
  {
    "id": "google_analytics_key",
    "label": "Google Analytics Tracker ID",
    "type": "text",
    "placeholder": "UA-34343-3"
  }]
}
